{
  "name": "@decky.fx/react-native-essentials",
  "version": "1.0.3",
  "description": "Collection of Zero Dependencies of usefull react/react-native functions",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prettier-write": "npx prettier --write",
    "prettier-check": "npx prettier --check",
    "public": "npm publish --access public"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://deckyfx@github.com/deckyfx/react-native-esentials.git"
  },
  "keywords": [
    "react",
    "react-native"
  ],
  "author": "Decky Fx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deckyfx/react-native-esentials/issues"
  },
  "homepage": "https://github.com/deckyfx/react-native-esentials#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {}
}
